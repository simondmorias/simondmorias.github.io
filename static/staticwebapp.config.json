{
  "routes": [
    {
      "route": "https://datathirst.dev/*",
      "redirect": "https://datathirst.net/*",
      "statusCode": 301
    },
    {
      "route": "/projects/dbfs-explorer",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/projects",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/services",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/contact",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/blog/tag/*",
      "rewrite": "/tags/"
    },
    {
      "route": "/blog/",
      "redirect": "/",
      "statusCode": 301
    }
  ],
  "trailingSlash": "auto",
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}